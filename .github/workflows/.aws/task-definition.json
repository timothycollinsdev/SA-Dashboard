{
    "containerDefinitions": [
        {
            "name": "nginx",
            "image": "878744080582.dkr.ecr.us-east-1.amazonaws.com/test-project-1:v0.0.33-alpha",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "ECS_IMAGE_PULL_BEHAVIOR",
                    "value": "once"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "stopTimeout": 2,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/test-task-1",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost/ || exit 1"
                ],
                "interval": 5,
                "timeout": 4,
                "retries": 3
            }
        }
    ],
    "family": "test-task-1",
    "taskRoleArn": "arn:aws:iam::878744080582:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::878744080582:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
